{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true,
          "modules": true,
          "experimentalObjectRestSpread": true
        }
      },
    "extends": [
      "eslint-config-airbnb"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "react/sort-comp": 0,
      "react/require-default-props": 0,
      "react/no-array-index-key": 0,
      "import/prefer-default-export": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/label-has-for": 0,
      "no-underscore-dangle": 0,
      "linebreak-style": 0,
      "consistent-return": 0,
      "global-require": 0,
      "no-multi-spaces": [
        "error",
        {
          "exceptions": {
            "Property": true,
            "ImportDeclaration": true,
            "VariableDeclarator": true
          }
        }
      ],
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "List",
            "Map",
            "Set"
          ]
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "spaced-comment": 0,
      "arrow-body-style": 0,
      "react/no-danger": 0,
      "react/no-unused-prop-types": 0,
      "react/no-multi-comp": 0,
      "react/forbid-prop-types": 0,
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/default": 0,
      "import/no-duplicates": 0,
      "import/named": 0,
      "import/namespace": 0,
      "import/no-unresolved": 0,
      "import/no-named-as-default": 2,
      "import/no-extraneous-dependencies": 0,
      "comma-dangle": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-shadow": 0,
      "no-plusplus": 0,
      "func-names": 0,
      "no-console": 0,
      "no-alert": 0,
      "max-len": 0,
      "no-unused-vars": 1,
      "import/extensions": 0
    },
    "plugins": [
      "react",
      "import"
    ],
    "globals": {
      "__DEVELOPMENT__": true,
      "__DEVTOOLS__": true
    }
  }
  